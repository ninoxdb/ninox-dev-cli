{
  "name": "@ninox/ninox",
  "description": "Ninox Database CLI for developer workflows",
  "version": "0.1.2",
  "author": "Muhammad Saqib Arfeen<saqib.arfeen@ninox.com>",
  "bin": {
    "ninox": "./bin/run.js"
  },
  "bugs": "https://github.com/ninoxdb/ninox-dev-cli/issues",
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@salesforce/kit": "^3.1.1",
    "axios": "^1.6.8",
    "debug": "^4.3.4",
    "form-data": "^4.0.0",
    "ora": "^8.0.1",
    "tty-table": "^4.2.3",
    "yaml": "^2.4.5",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/debug": "^4.1.12",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10",
    "nock": "^13.5.4",
    "oclif": "^4",
    "shx": "^0.3.3",
    "sinon": "^17.0.2",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ninoxdb/ninox-dev-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "ninox",
    "dirname": "ninox",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "topics": {}
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninoxdb/ninox-dev-cli.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "readme": "oclif readme",
    "prepack": "oclif manifest && oclif readme",
    "test:integration": "mocha --forbid-only \"test/commands/**/*.test.ts\"",
    "test": "mocha --forbid-only \"src/**/*.spec.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
