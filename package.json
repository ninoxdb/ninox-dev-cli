{
  "name": "ninox-cli",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "npx --no-install prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "npx --no-install eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "NODE_ENV=test npx --no-install jest --verbose --config jest-unit.config.ts",
    "test:integration": "NODE_ENV=test npx --no-install jest --forceExit --config jest-intgration.config.ts",
    "test:watch": "npx --no-install jest --watch",
    "test:cov": "npx --no-install jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "npx --no-install husky install"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@faker-js/faker": "^8.3.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport": "^1.0.16",
    "@types/passport-http": "^0.3.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "commitlint-config-jira-jquery": "^1.0.6",
    "commitlint-plugin-shr-jira": "^1.0.6",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "faker": "^6.6.6",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "=20.11.0",
    "npm": "=10.4.0"
  }
}
